{
  "name": "tenzu",
  "version": "0.1.1",
  "schematics": "schematics/collection.json",
  "scripts": {
    "ng": "ng",
    "start": "node ./init-config.js  && ng serve  tenzu --hmr --host 0.0.0.0 --allowed-hosts true",
    "build": "ng build && npm run sentry:sourcemaps",
    "build-schematics": "./node_modules/.bin/tsc -p ./tsconfig.schematics.json",
    "schematics:add-plugins": "schematics ./src/schematics/collection.json:add-plugins --dry-run=false",
    "schematics:reset-plugins": "schematics ./src/schematics/collection.json:reset-plugins --dry-run=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng --no-watch --code-coverage",
    "test:watch": "ng test  --watch",
    "e2e:ui": "npx playwright test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "transloco:optimize": "transloco-optimize dist/tenzu/browser/assets/i18n",
    "build:staging": "ng build --configuration staging && npm run transloco:optimize",
    "build:production": "ng build --configuration production && npm run transloco:optimize",
    "serve-prod-local": "npx http-server dist/tenzu/browser -p 4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/cdk": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/material": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@blocknote/code-block": "^0.32.0",
    "@blocknote/core": "^0.32.0",
    "@blocknote/mantine": "^0.32.0",
    "@blocknote/react": "^0.32.0",
    "@jsverse/transloco": "^7.6.1",
    "@jsverse/transloco-locale": "^7.0.1",
    "@jsverse/transloco-messageformat": "^7.0.1",
    "@ngrx/operators": "^20.0.0",
    "@ngrx/signals": "^20.0.0",
    "@sentry/angular": "^10.5.0",
    "file-saver-es": "^2.0.5",
    "ng-event-bus": "^9.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.0.3",
    "zod": "^3.25.61"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^20.1.5",
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@eslint/js": "^10.0.0",
    "@jsverse/transloco-optimize": "^7.0.1",
    "@ngrx/eslint-plugin": "^20.0.0",
    "@sentry/cli": "^2.52.0",
    "@types/file-saver-es": "^2.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "angular-eslint": "^20.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "http-serve": "^1.0.1",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
