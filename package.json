  "scripts": {
    "ng": "ng",
    "start": "ng serve  tenzu --hmr --host local-tenzu.biru.ovh",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng --no-watch --code-coverage",
